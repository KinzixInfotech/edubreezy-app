{
  "cli": {
    "version": ">= 16.24.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "node": "18",
      "distribution": "internal",
      "env": {
        "NPM_CONFIG_FORCE": "true"
      }
    },
    "production": {
      "node": "18",
      "autoIncrement": true,
      "env": {
        "NPM_CONFIG_FORCE": "true"
      }
    }
  },
  "submit": {
    "production": {}
  }
}